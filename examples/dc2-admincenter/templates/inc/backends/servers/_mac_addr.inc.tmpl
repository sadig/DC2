<div class='container'>
    <div class='row'>
        <div class='span12'>
            <table class='table table-bordered tabled-striped'>
                <thead>
                    <tr>
                        <th>MAC Address</th>
                        <th>Device Name</th>
                    </tr>
                </thead>
                <tbody>
			{%- for mac in macs %}
			{{ mac }}
                    <tr>
                        <td>{{mac.mac_addr}}</td>
                        <td>{{mac.device_name}}</td>
                    </tr>
                    {%- endfor %}
                </tbody>
            </table>
        </div>
    </div>
</div>
